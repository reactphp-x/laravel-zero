{
    "name": "reactphp-x/reactphp-x",
    "description": "The Reactphp Framework build top of Framework X and Larave zero.",
    "keywords": ["Reactphp", "framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://reactphp-x.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/reactphp-x/reactphp-x/issues",
        "source": "https://github.com/reactphp-x/reactphp-x"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.0.0",
        "reactphp-x/crontab": "^1.0",
        "reactphp-x/laravel-log": "^1.0",
        "reactphp-x/laravel-reactphp": "^1.0",
        "reactphp-x/orm": "^2.0@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["artisan"]
}
